{
  "name": "cypress-cucumber-test",
  "version": "1.0.0",
  "description": "cypress with cucumber",
  "main": "index.js",
  "scripts": {
    "test": "cypress run  --headed spec \"**/*.features\"",
    "test:all": "cypress run --spec \"**/*.features\""
  },
  "dependencies": {
    "cypress": "^3.4.1",
    "cypress-cucumber-preprocessor": "^1.16.2"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  }
}
